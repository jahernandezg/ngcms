  "license": "MIT",
  "scripts": {
    "start:frontend": "nx serve frontend",
    "build:frontend": "nx build frontend",
  "build:frontend:ssr": "nx build frontend --configuration=production",
  "serve:frontend:ssr": "node dist/frontend/server/server.mjs",
    "start:backend": "nx serve backend",
    "start:backend:ps": "nx run backend:serve-winps",
    "build:backend": "nx build backend",
    "test": "nx test",
    "test:ci": "nx run-many -t test",
    "test:coverage": "nx run-many -t test --coverage",
    "lint": "nx lint",
    "e2e": "nx e2e frontend-e2e",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "node prisma/seed.cjs",
    "menu:show": "node scripts/show-menu.cjs",
    "bootstrap": "npm run prisma:migrate && npm run prisma:seed",
    "postinstall": "npx prisma generate",
    "prepare": "husky install",
    "openapi:build": "nx build backend",
    "openapi:gen": "npm run openapi:build && node backend/scripts/generate-openapi.cjs",
    "openapi:types": "npm run openapi:gen && openapi-typescript swagger.json --output libs/shared-types/src/lib/openapi-types.ts",
    "openapi:all": "npm run openapi:types",
    "user:create": "node prisma/create-user.cjs",
    "user:create:env": "node prisma/create-user.cjs --email=$USER_EMAIL --name=\"$USER_NAME\" --password=$USER_PASSWORD --roles=$USER_ROLES",
    "perf:admin": "ts-node -P backend/tsconfig.perf.json backend/src/scripts/perf.seed-and-test.ts"
  },
  "private": true,
